{
  "schemaVersion": 1,
  "id": "jublockly",
  "version": "${version}",
  "name": "jublockly",
  "description": "",
  "authors": [],
  "contact": {
    "repo": "https://github.com/arctic-pot/jublockly"
  },
  "license": "MIT",
  "icon": "assets/jublockly/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.arcticpot.jublockly.client.JublocklyClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.arcticpot.jublockly.Jublockly"
      }
    ]
  },
  "mixins": [
    "jublockly.mixins.json"
  ],
  "depends": {
    "fabric-language-kotlin": ">=1.9.4+kotlin.1.8.21",
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
